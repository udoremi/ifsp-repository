algoritmo "tabuada"

var
contador, resp, numero:inteiro
inicio
 escreva("Informe o número: ")
 leia(numero)
 
 para contador <- 1 ate 10 faca
 //(ou) resp <- numero * contador
   //se(numero<>2)entao
 escreval(numero, " X ", contador, " = ", numero * contador)
   //fimse
 fimpara


fimalgoritmo
